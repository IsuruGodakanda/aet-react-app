{
  "name": "aet-react-app",
  "version": "1.0.0",
  "description": "Use typescript in create-react-app project as well as adding it to a react project from scratch with webpack and babel. And also includes about react hooks, (useState, useContext, useReducer and useEffect), in a react typescript project as well as using suspense to lazy load a component.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env platform=dev NODE_ENV=development",
    "start-staging": "webpack serve --env platform=staging NODE_ENV=development",
    "start-prod": "webpack serve --env platform=prod NODE_ENV=development",
    "build": "webpack --env platform=dev NODE_ENV=production",
    "build-staging": "webpack --env platform=staging NODE_ENV=production",
    "build-prod": "webpack --env platform=prod NODE_ENV=production",
    "serve": "rm -rf build && yarn build && serve build/dist",
    "serve-staging": "rm -rf build && yarn build-staging && serve build/dist",
    "serve-prod": "rm -rf build && yarn build-prod && serve build/dist",
    "restart": "yarn reboot; yarn start",
    "reboot": "rm -rf node_modules; yarn install",
    "deploy": "rm -rf build && yarn build && aws s3 sync build/dist/ s3://BUCKET_NAME --acl public-read",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest",
    "test-watch": "jest --watch",
    "clean": "rm -rf build coverage .nyc_output",
    "clean-node": "rm -rf node_modules"
  },
  "repository": "https://github.com/IsuruGodakanda/aet-react-app",
  "author": "Aeturnum-Isuru-Godakanda",
  "license": "ISC",
